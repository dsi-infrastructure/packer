{
  "provisioners": [
    {
      "type": "shell",
      "execute_command": "echo 'vagrant'|sudo -S sh '{{.Path}}'",
      "override": {
        "virtualbox-iso": {
          "scripts": [
            "scripts/centos/base.sh",
            "scripts/centos/vagrant.sh",
            "scripts/centos/chef-client.sh",
            "scripts/centos/virtualbox-tools.sh",
            "scripts/centos/cleanup.sh",
            "scripts/centos/zerodisk.sh"
          ]
        }
      }
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "override": {
        "virtualbox": {
          "output": "redhat-5-dev-virtualbox-chef.box"
        }
      }
    }
  ],
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
      	"<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/redhat-dev.kickstart<enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": 10240,
      "guest_os_type": "RedHat_64",
      "http_directory": "http",
      "iso_url": "http://localhost/~sitle/rhel5.4.iso",
      "iso_checksum": "04fe3c10202402d7b389528d2bad0210",
      "iso_checksum_type": "md5",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "echo 'vagrant'|sudo -S /sbin/shutdown -hP now",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "virtualbox_version_file": ".vbox_version",
      "vboxmanage": [
        [ "modifyvm", "{{.Name}}", "--memory", "512" ],
        [ "modifyvm", "{{.Name}}", "--cpus", "1" ]
      ]
    }
  ]
}
